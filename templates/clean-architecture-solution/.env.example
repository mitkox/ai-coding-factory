# Required database settings for docker-compose
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=ProjectName

# Optional pgAdmin profile (docker-compose --profile tools up)
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=

# Optional JWT settings (if enabling auth)
JWT__Secret=
JWT__Issuer=ProjectName
JWT__Audience=ProjectName
